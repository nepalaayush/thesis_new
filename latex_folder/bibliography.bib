	number = {6},
	journaltitle = {Annals of the Rheumatic Diseases},
	shortjournal = {Ann Rheum Dis},
	author = {Bennell, Kim L and Creaby, Mark W and Wrigley, Tim V and Bowles, Kelly-Ann and Hinman, Rana S and Cicuttini, Flavia and Hunter, David J},
	urldate = {},
	date = {2010-06},
	langid = {english},
}


