	author = {Fuchs, Andreas and Georgii, Joachim and Taghizadeh, Elham and Heldmann, Stefan and Lange, Thomas and Bendak, Sebastian F. and Siegel, Markus and Yilmaz, Tayfun and Schmal, Hagen and Izadpanah, Kaywan},
